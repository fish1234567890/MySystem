<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.xyz.MySystem.dao.interf.UserMapper">
	<select id="getUser" resultType="userbaseinfo">
		select * from com_user_baseinfo where 1=1 and email = #{arg0} and password = #{arg1}
	</select>
</mapper>